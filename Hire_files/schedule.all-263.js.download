function isEmailValid(email) {
	// multiple e-mails can be entered, make sure there's at least the hint of _one_ valid
    var re = /([^@]+)@((?:[\w-]+\.)*\w[\w-]{0,66})\.([a-z]{2,20}(?:\.[a-z]{2})?)/i;
    return re.test(email.trim());
