function formatTimeForHuman(d)
{
	if (!d) return '';
	
	if (typeof d == 'string') {
		if (d.toLowerCase().indexOf('am')>-1 || d.toLowerCase().indexOf('pm')>-1 || d.toLowerCase().indexOf('a.m.')>-1 || d.toLowerCase().indexOf('p.m.')>-1) {
